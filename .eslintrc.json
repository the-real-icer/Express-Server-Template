{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "airbnb-base"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": 0,
        "linebreak-style": 0,
        "comma-dangle": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "object-curly-newline": 0,
        "camelcase": 0,
        "no-restricted-syntax": 1,
        "import/extensions": [2, { "js": "always" }],
        "import/no-commonjs": 2,
        "operator-linebreak": 0
    }
}
